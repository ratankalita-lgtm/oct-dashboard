  {
    "id": "dme",
    "name": "Diabetic Macular Edema (DME)",
    "aliases": ["ci-dme", "csme", "diabetic edema"],
    "domain": "Retina",
    "category": "Vascular",
    "oct": [
      "Center involvement: foveal IRF and/or central thickening.",
      "Pattern: focal vs diffuse; note SRF if present.",
      "Prognosis markers: DRIL, EZ/ELM disruption, hyperreflective foci, exudate shadowing.",
      "Check traction (ERM/VMT): if present, medical response may be limited.",
      "Minimum documentation: CST trend, IRF/SRF presence, EZ status"
    ],
    "octa": [
      "FAZ enlargement/irregularity → ischemic maculopathy risk.",
      "SCP/DCP dropout (DCP often earlier).",
      "Artifact checks: motion, projection, segmentation errors."
    ],
    "targets": [
      "Center-involving DME + vision impact → Anti-VEGF pathway.",
      "Tractional component (ERM/VMT) → retina opinion.",
      "Inadequate response to Anti-VEGF → consider steroid pathway.",
      "Optimize systemic factors: HbA1c, BP, lipids, renal status."
    ],
    "red_flags": [
      "Large FAZ enlargement/marked DCP dropout with poor vision (ischemic maculopathy).",
      "Tractional DME (ERM/VMT) with distortion.",
      "New NVE/NVD suspicion; vitreous hemorrhage symptoms."
    ],
    "follow_up": "Active treatment: 4-6 weeks. Stable: 6-10 weeks.",
    "triage": "Treat now: CI-DME with ↓vision. Refer early if tractional signs.",
    "ddx": ["Post-operative CME", "Uveitic macular edema", "RVO-related macular edema", "Tractional macular edema (ERM/VMT)", "CSC (SRF-dominant)"],
    "ddx_rules": [
      {
        "candidate": "Post-operative CME",
        "reason": "Recent surgery + CME pattern",
        "weight": 3,
        "when": { "flag": "recent_surgery", "is": true }
      }
    ]
  },
